/*---------------------     Basic Animations      --------------*/

@keyframes fade-in-out {
	from {
		opacity: 0;
		visibility: visible;
	}

	50% {
		opacity: 1;
	}

	to {
		opacity: 0;
	}
}

@keyframes fade-out-in {
	from {
		opacity: 1;
	}

	50% {
		opacity: 0;
	}

	to {
		opacity: 1;
	}
}

@keyframes fade-in {
	from {
		opacity: 0;
	}

	to {
		opacity: 1;
	}
}

@keyframes fade-out {
	from {
		opacity: 1;
	}


	to {
		opacity: 0;
	}
}

