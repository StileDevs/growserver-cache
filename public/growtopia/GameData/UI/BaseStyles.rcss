/*----------- LOADER ANIMATION ------------*/
.loader.hidden {
    display: none;
}

.loader {
  position: relative;
  width: 32px;
  height: 32px;
  border-radius: 10px;
}

.loader div {
  width: 2%;
  height: 20%;
  background: rgb(255,255,255);
  position: absolute;
  left: 50%;
  top: 30%;
  opacity: 0;
  border-radius: 50px;
  border: 1dp white;
}

@keyframes loaderfade {
  from {
    opacity: 1;
  }

  to {
    opacity: 0.25;
  }
}

.loader .bar1 {
  transform: rotate(0deg) translate(0, -130%);
  animation: 1.1s linear infinite loaderfade;
}

.loader .bar2 {
  transform: rotate(30deg) translate(0, -130%);
  animation: 1.1s 0.1s linear infinite loaderfade;
}

.loader .bar3 {
  transform: rotate(60deg) translate(0, -130%);
  animation: 1.1s 0.2s linear infinite loaderfade;
}

.loader .bar4 {
  transform: rotate(90deg) translate(0, -130%);
  animation: 1.1s 0.3s linear infinite loaderfade;
}

.loader .bar5 {
  transform: rotate(120deg) translate(0, -130%);
  animation: 1.1s 0.4s linear infinite loaderfade;
}

.loader .bar6 {
  transform: rotate(150deg) translate(0, -130%);
  animation: 1.1s 0.5s linear infinite loaderfade;
}

.loader .bar7 {
  transform: rotate(180deg) translate(0, -130%);
  animation: 1.1s 0.6s linear infinite loaderfade;
}

.loader .bar8 {
  transform: rotate(210deg) translate(0, -130%);
  animation: 1.1s 0.7s linear infinite loaderfade;
}

.loader .bar9 {
  transform: rotate(240deg) translate(0, -130%);
  animation: 1.1s 0.8s linear infinite loaderfade;
}

.loader .bar10 {
  transform: rotate(270deg) translate(0, -130%);
  animation: 1.1s 0.9s linear infinite loaderfade;
}

.loader .bar11 {
  transform: rotate(300deg) translate(0, -130%);
  animation: 1.1s 1s linear infinite loaderfade;
}

.loader .bar12 {
  transform: rotate(330deg) translate(0, -130%);
  animation: 1.1s 1.1s linear infinite loaderfade;
}
/* -------- SELECT --------*/

select selectarrow {
    decorator: image(interface/large/gui_auction_dropdown_arrow.rttex);
    width: 40dp;
    height: 20dp;
	box-shadow: #000 0 2dp 2dp 0dp;
}

select:active selectarrow,
select selectarrow:checked {
    transform: rotate(180deg);
	box-shadow: #000 0 -2dp 2dp 0dp;
}

selectbox {
	border: 4dp rgba(135, 184, 204, 255);
    background-color: rgb(80, 157, 188);
    border-radius: 5dp;
    margin-top: -5dp;
    margin-right: 48dp;
    padding: 3dp;
}

select {
    display: block;
	vertical-align: middle;
    width: 410dp;
    height: 40dp;
    margin: 0 0dp 0 5dp;
	border: 4dp rgba(135, 184, 204, 255);
    background-color: rgba(135, 184, 204, 150);
    border-radius: 5dp;
	padding: 3dp 2dp 3dp 7dp;
    font-size: 20dp;
    font-weight: bold;
    line-height: 30dp;
	color: #fff;
    font-effect: shadow(1dp 1dp #000);
	box-shadow: #000 1dp 1dp 2dp 1dp inset, #000 1dp 1dp 2dp 1dp;
	sound-on-click: true;
}

select.disabled {
	sound-on-click: false;
}

option {
	padding: 5dp;
    white-space: nowrap;
    font-effect: shadow(1dp 1dp #000);
}

option.selected {
    background-color: rgb(135, 184, 204);
}
truncatingstring {
	display: block;
}