/*-------------------------------------------------------------------------------------------*/
/*------------------------              Button Style              ---------------------------*/

@spritesheet button-sprites {
    src: game/gui_box_upwhite.rttex;
    resolution: 1x;
    button-bg: 0px 0px 29px 29px;
    button-bg-inner: 15px 15px 2px 2px;
}

.img-button {
    decorator: ninepatch(button-bg, button-bg-inner);
	display: inline-block;
    padding: 3dp 15dp 5dp 10dp; /* padding-top, padding-right, padding-bottom, padding-left */
	text-align: center;
	vertical-align: middle;
	align-items: center;
	justify-content: center;
	sound-on-click: true;
}

.button {
	display: inline-block;
	padding: 10dp;
	border-radius: 6dp;
	text-align: center;
	vertical-align: middle;
	justify-content: center;
	box-shadow: #000f 2dp 2dp 2dp 0px;
	sound-on-click: true;
}

	.button:active, .img-button:active {
		filter: brightness(0.85);
		transform: scale(0.95);
	}

.button.disabled, .img-button.disabled {
    filter: grayscale(1) opacity(0.7) brightness(0.85);
	sound-on-click: false;
}

.button.disabled:active, .img-button.disabled:active {
    filter: brightness(0.85) opacity(0.7) grayscale(1);
    transform: scale(1);
	pointer-events: none;
}

	.button .label, .img-button .label {
		display: inline-block;
		color: #fff;
		font-family: Century Gothic;
		font-size: 30dp;
		text-align: center;
		white-space: nowrap;
		width: auto;
		font-effect: shadow(1dp 1dp #000);
	}

	.button .button-icon , .img-button .button-icon{
		vertical-align: middle;
		margin-right: 12dp;
		width: 32dp;
		height: 32dp;
	}

.label {
	line-height: 0.78em;
	margin-top: 0.2em;
	margin-bottom: 0.28em;
}

