<template name="AuctionListItem">
  <head>
    <link type="text/rcss" href="../../rml.rcss" />
    <link type="text/rcss" href="../../BaseStyles.rcss" />
    <link type="text/rcss" href="AuctionListItem.rcss" />
  </head>
  <body>
    <div class="message auction">
      <div class="col-left">
        <div class="icon">
          <span class="image" data-style-decorator="'itemidimage(' + item_icon +')'"/>
        </div>
      </div>
      <div class="col-middle">
        <div class="split">
          <div class="title font-body1">{{ItemName(item_icon)}}</div>
          <div class="wl-count font-body1">
            <span class="color_yellow">{{MoneyString(current_bid, 1)}} WL</span>
          </div>
        </div>
      </div>
      <div class="col-right">
        <div class="split">
          <div data-attr-message_index="it_index" onclick="OnEvent(event, AuctionCellActionName)" class="img-button button-default-color" data-class-hidden="!can_warp">
            <span class="label">Go To World</span>
          </div>
          <div data-attr-message_index="it_index" class="img-button button-default-color disabled" data-class-hidden="can_warp">
            <span class="label">Current World</span>
          </div>
          <div class="timer-text font-body2 color_dreamsicle">
            <Timer data-attr-end-epoch="end_time" lowest-unit="m">
              <active>
                <span class="color_dreamsicle">Expires In: </span>
                <timertext class="color_pinky_purple"/>
              </active>
              <expired>Expired!</expired>
            </Timer>
          </div>
          
        </div>
      </div>
    </div>
  </body>
</template>